{
  "name": "flask-datta-able-base",
  "version": "1.0.0",
  "description": "flask-datta-able-base template for rapid prototyping and POCs",
  "main": "app.py",
  "scripts": {
    "build-css": "sass --watch static/scss/main.scss:static/css/style.css",
    "build-css-prod": "sass static/scss/main.scss:static/css/style.css --style=compressed",
    "dev": "concurrently \"python app.py\" \"npm run build-css\"",
    "install-deps": "pip install -r requirements.txt && npm install"
  },
  "keywords": ["flask", "htmx", "bootstrap", "datta-able", "admin", "template"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.69.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "@popperjs/core": "^2.11.8",
    "bootstrap-icons": "^1.11.0"
  }
}